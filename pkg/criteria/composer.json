{
  "name": "getwarp/criteria",
  "description": "Define rules to filter datasets in declarative way",
  "type": "library",
  "keywords": [
    "criteria",
    "filter",
    "match",
    "pagination"
  ],
  "homepage": "https://github.com/getwarp/criteria",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://getwarp.dev",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "symfony/property-access": "^4.4|^5.0",
    "webmozart/assert": "^1.6",
    "webmozart/expression": "^1.0"
  },
  "require-dev": {
    "doctrine/collections": "^1.6.4",
    "roave/security-advisories": "dev-latest",
    "spiral/pagination": "^2.7"
  },
  "suggest": {
    "spiral/pagination": ""
  },
  "autoload": {
    "psr-4": {
      "Warp\\Criteria\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Warp\\Criteria\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
