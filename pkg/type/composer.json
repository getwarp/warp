{
  "name": "getwarp/type",
  "description": "Runtime type checking and casting in PHP",
  "type": "library",
  "keywords": [
    "type",
    "type-check",
    "cast"
  ],
  "homepage": "https://github.com/getwarp/type",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://getwarp.dev",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.2|^8.0",
    "webmozart/assert": "^1.6"
  },
  "require-dev": {
    "ext-json": "*",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "files": [
      "src/polyfill.php"
    ],
    "psr-4": {
      "Warp\\Type\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Warp\\Type\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "replace": {
    "spaceonfire/type": "^2.5"
  }
}
