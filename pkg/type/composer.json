{
  "name": "getwarp/type",
  "description": "Runtime type checking and casting in PHP",
  "type": "library",
  "keywords": [
    "type",
    "type-check",
    "cast"
  ],
  "homepage": "https://github.com/getwarp/type",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://getwarp.dev",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "getwarp/common": "^3.1"
  },
  "require-dev": {
    "ext-json": "*",
    "phpspec/prophecy": "^1.13",
    "phpspec/prophecy-phpunit": "^2.0",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "autoload": {
    "psr-4": {
      "Warp\\Type\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Warp\\Type\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
