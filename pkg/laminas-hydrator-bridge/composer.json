{
  "name": "getwarp/laminas-hydrator-bridge",
  "description": "Integration for Laminas Hydrator with other Warp components",
  "type": "library",
  "keywords": [
    "warp",
    "laminas",
    "laminas-hydrator",
    "hydrator"
  ],
  "homepage": "https://github.com/getwarp/laminas-hydrator-bridge",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://getwarp.dev",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "laminas/laminas-hydrator": "^4.0"
  },
  "require-dev": {
    "getwarp/data-source": "^3.1",
    "getwarp/type": "^3.1",
    "getwarp/value-object": "^3.1",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-latest"
  },
  "suggest": {
    "getwarp/type": "Used with CasterStrategy",
    "getwarp/value-object": "Used with ValueObjectStrategy",
    "getwarp/data-source": "Used with BlameStrategy"
  },
  "autoload": {
    "psr-4": {
      "Warp\\Bridge\\LaminasHydrator\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Warp\\Bridge\\LaminasHydrator\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
