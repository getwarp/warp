{
  "name": "getwarp/command-bus",
  "description": "Simple and extendable command bus",
  "type": "library",
  "keywords": [
    "command-bus",
    "query-bus",
    "message-bus",
    "command",
    "query"
  ],
  "homepage": "https://github.com/getwarp/command-bus",
  "license": "MIT",
  "authors": [
    {
      "name": "Constantine Karnaukhov",
      "email": "genteelknight@gmail.com",
      "homepage": "https://getwarp.dev",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "^7.4|^8.0",
    "getwarp/container": "^3.1",
    "getwarp/exception": "^3.1",
    "yiisoft/friendly-exception": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "psr/log": "^1.1",
    "roave/security-advisories": "dev-latest",
    "symfony/stopwatch": "^5.2|^6.0"
  },
  "suggest": {
    "psr/log-implementation": "Required for LoggerMiddleware and ProfilerMiddleware",
    "symfony/stopwatch": "Required for ProfilerMiddleware"
  },
  "autoload": {
    "psr-4": {
      "Warp\\CommandBus\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Warp\\CommandBus\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
